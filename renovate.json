{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":semanticCommits",
    "mergeConfidence:all-badges"
  ],
  "kubernetes": {
    "managerFilePatterns": [
      "/^.*deployment\\.yaml$/",
      "/^.*cronjob\\.yaml$/"
    ]
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "linuxserver/qbittorrent"
      ],
      "matchDatasources": [
        "docker"
      ],
      "allowedVersions": "!/.*(14\\.3\\.9|20\\.04\\.1).*/"
    }
  ]
}
